<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Playwright Suite" parallel="tests" thread-count="3">
    <test name="Playwright Tests - Chromium">
        <parameter name="browserName" value="chromium"/>
        <classes>
            <class name="org.example.PlaywrightTest"/>
        </classes>
    </test>
    <test name="Playwright Tests - Firefox">
        <parameter name="browserName" value="firefox"/>
        <classes>
            <class name="org.example.PlaywrightTest"/>
        </classes>
    </test>
    <test name="Playwright Tests - WebKit">
        <parameter name="browserName" value="webkit"/>
        <classes>
            <class name="org.example.PlaywrightTest"/>
        </classes>
    </test>
    <test name="Playwright Tests - Edge">
        <parameter name="browserName" value="edge"/>
        <classes>
            <class name="org.example.PlaywrightTest"/>
        </classes>
    </test>
</suite>
